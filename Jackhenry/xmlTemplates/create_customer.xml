<?xml version="1.0" encoding="UTF-8"?>
<SOAP-ENV:Envelope
    xmlns:SOAP-ENV="http://schemas.xmlsoap.org/soap/envelope/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <SOAP-ENV:Header>
        <wsse:Security
            xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd"
            xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd">
            <wsse:UsernameToken
                xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
                <wsse:Username>{{XUserName}}</wsse:Username>
                <wsse:Password
                    Type="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-username-token-profile-1.0#PasswordText">
                    {{XPassword}}</wsse:Password>
                <wsu:Created
                    xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd">
                    {{current_timestamp}}</wsu:Created>
            </wsse:UsernameToken>
        </wsse:Security>
    </SOAP-ENV:Header>
    <SOAP-ENV:Body>
        <CustAdd
            xmlns="http://jackhenry.com/jxchange/TPG/2008">
            <MsgRqHdr>
                <jXchangeHdr>
                    <JxVer />
                    <AuditUsrId>{{XAuditUser}}</AuditUsrId>
                    <AuditWsId>{{XAuditWorkStation}}</AuditWsId>
                    <AuthenUsrId />
                    <ConsumerName />
                    <ConsumerProd />
                    <Ver_1 />
                    <jXLogTrackingId>{{guid}}</jXLogTrackingId>
                    <Ver_2 />
                    <InstRtId>{{XInstitutionRoutingID}}</InstRtId>
                    <InstEnv>{{XEnvironment}}</InstEnv>
                    <Ver_3 />
                    <BusCorrelId>{{guid}}</BusCorrelId>
                    <Ver_4 />
                    <WorkflowCorrelId />
                    <Ver_5 />
                    <ValidConsmName>{{XValidConsumerName}}</ValidConsmName>
                    <ValidConsmProd>{{XValidConsumerProduct}}</ValidConsmProd>
                    <Ver_6 />
                </jXchangeHdr>
            </MsgRqHdr>
            <ErrOvrRdInfoArray>
                {% for ErrOvrRd in ErrOvrRds %}
                <ErrOvrRd>
                    <ErrCode>410023</ErrCode>
                    <Ver_1 />
                </ErrOvrRd>
                {% endfor %}
            </ErrOvrRdInfoArray>
            <CustDetail>
                <PersonName>
                    <ComName>{{ComName}}</ComName>
                    {% if entity_type == "individual" %}
                    <FirstName>{{FirstName}}</FirstName>
                    <MiddleName>{{MiddleName}}</MiddleName>
                    <LastName>{{LastName}}</LastName>
                        <NameSuffix>{{NameSuffix}}</NameSuffix>
                    </x_PersonName>
                    {% endif %}
                    {% if entity_type == "entity" %}
                    <x_PersonName>
                        <LegalName>{{LegalName}}</LegalName>
                    </x_PersonName>
                    {% endif %}
                </PersonName>
                <Addr>
                    <StreetAddr1>{{StreetAddr1}}</StreetAddr1>
                    <StreetAddr2>{{StreetAddr2}}</StreetAddr2>
                    <City>{{City}}</City>
                    <StateCode>{{StateCode}}</StateCode>
                    <PostalCode>{{PostalCode}}</PostalCode>
                    <DlvryPt>123456</DlvryPt>
                </Addr>
                <CustType>N</CustType>
                
                {% if entity_type == "entity" or ISSOLEPROP == true %}
                <NAICSCode>111940</NAICSCode>
                {% endif %}

                {% if entity_type == "individual" %}
                <BirthDt>1985-06-15</BirthDt>
                {% endif %}

                {% if ISSOLEPROP == true %}
                <EmplName>XYZ Industries</EmplName>
                {% endif %}

                <EmailArray>
                    <EmailInfo>
                        <EmailAddr>pkumar+42309@thesummitgrp.com</EmailAddr>
                        <EmailType>Primary E-Mail</EmailType>
                    </EmailInfo>
                </EmailArray>
                <PhoneArray>
                    <PhoneInfo>
                        <PhoneType>Business Phone</PhoneType>
                        <Ver_1 />
                        <Ver_2 />
                        <Ver_3 />
                        <Ver_4 />
                        <PhoneNum>2342342342</PhoneNum>
                    </PhoneInfo>
                </PhoneArray>
                <CrScoreArray>
                    <CrScoreInfo>
                        <CrScoreId>212</CrScoreId>
                        {% if entity_type == "individual" %}
                        <CertCode>C</CertCode>
                        <CertCodeDt>2025-08-06</CertCodeDt>
                        {% endif %}
                    </CrScoreInfo>
                </CrScoreArray>
            </CustDetail>
            <TaxDetail>
                <TINInfo>
                    <TINCode>B</TINCode>
                    <TaxId>987691031</TaxId>
                    {% if entity_type == "individual" %}
                    <CertCode>C</CertCode>
                    <CertCodeDt>2025-08-06</CertCodeDt>
                    {% endif %}
                </TINInfo>
                {% if entity_type == "individual" %}
                <Alien>
                    <CitzCntryType>US</CitzCntryType>
                </Alien>
                {% endif %}
                <FedWithCode>0</FedWithCode>
                <StateWithCode>0</StateWithCode>
            </TaxDetail>
            <BusDetail>
                <BrCode>51</BrCode>
                <OffCode>BNO</OffCode>
                <CustCode>B</CustCode>
                {% if entity_type == "individual" %}
                <InsiderCode>E</InsiderCode>
                <NetWorth>500000</NetWorth>
                {% endif %}
                <NetWorthAmt>500000</NetWorthAmt>
            </BusDetail>
            <RegDetail>
                <ReqLegalEntityType>true</ReqLegalEntityType>
                {% if entity_type == "individual" and ISSOLEPROP != true %}
                <BenflOwnExclCode>NP</BenflOwnExclCode>
                {% endif %}
                <AnnRevenueAmt>1200000</AnnRevenueAmt>
                <CRARec>
                    <CRAStateCode>2</CRAStateCode>
                    <CRACountyCode>170</CRACountyCode>
                    <CRACenTract>1.01</CRACenTract>
                    <CRASMSACode>11260</CRASMSACode>
                    <CRAIncmLvl>1</CRAIncmLvl>
                </CRARec>
            </RegDetail>
        </CustAdd>
    </SOAP-ENV:Body>
</SOAP-ENV:Envelope>