{
    "timeout": 30,
    "xsd_name": "TPG_CustomerMaster",
    "xml_template": "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<SOAP-ENV:Envelope xmlns:SOAP-ENV=\"http://schemas.xmlsoap.org/soap/envelope/\" xmlns:xsd=\"http://www.w3.org/2001/XMLSchema\" xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">\n    <SOAP-ENV:Header>\n        <wsse:Security xmlns:wsse=\"http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd\" xmlns:wsu=\"http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd\">\n            <wsse:UsernameToken xmlns:wsse=\"http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd\">\n                <wsse:Username>{{XUserName}}</wsse:Username>\n                <wsse:Password Type=\"http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-username-token-profile-1.0#PasswordText\">{{XPassword}}</wsse:Password>\n                <wsu:Created xmlns:wsu=\"http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd\"> {{current_timestamp}}</wsu:Created>\n            </wsse:UsernameToken>\n        </wsse:Security>\n    </SOAP-ENV:Header>\n    <SOAP-ENV:Body>\n        <CustSrch xmlns=\"http://jackhenry.com/jxchange/TPG/2008\">\n            <SrchMsgRqHdr>\n                <jXchangeHdr>\n                    <JxVer />\n                    <AuditUsrId>{{XAuditUser}}</AuditUsrId>\n                    <AuditWsId>{{XAuditWorkStation}}</AuditWsId>\n                    <Ver_1 />\n                    <jXLogTrackingId>{{jXLogTrackingId}}</jXLogTrackingId>\n                    <Ver_2 />\n                    <InstRtId>{{XInstitutionRoutingID}}</InstRtId>\n                    <InstEnv>{{XEnvironment}}</InstEnv>\n                    <Ver_3 />\n                    <BusCorrelId>{{BusCorrelId}}</BusCorrelId>\n                    <Ver_4 />\n                    <WorkflowCorrelId />\n                    <Ver_5 />\n                    <ValidConsmName>{{XValidConsumerName}}</ValidConsmName>\n                    <ValidConsmProd>{{XValidConsumerProduct}}</ValidConsmProd>\n                    <Ver_6 />\n                </jXchangeHdr>\n                <MaxRec>20</MaxRec>\n                <Cursor />\n                <Ver_1 />\n                <Ver_2 />\n                <Ver_3 />\n            </SrchMsgRqHdr>\n            <TaxId>{{CustSrch.TaxId}}</TaxId>\n            <PersonName>\n                <ComName>{{CustSrch.PersonName.ComName}}</ComName>\n            </PersonName>\n            <Ver_1 />\n            <Ver_2 />\n            <Ver_3 />\n            <Ver_4 />\n            <IncXtendElemArray>\n                <IncXtendElemInfo>\n                    <XtendElem>x_PersonName</XtendElem>\n                    <Ver_1 />\n                </IncXtendElemInfo>\n            </IncXtendElemArray>\n            <Ver_5 />\n            <Ver_6 />\n            <Ver_7 />\n        </CustSrch>\n    </SOAP-ENV:Body>\n</SOAP-ENV:Envelope>",
    "xml_error_path": "s:Envelope/s:Body/CustSrchResponse/SrchMsgRsHdr/MsgRecInfoArray/MsgRec/ErrDesc",
    "validation_spec": "{}",
    "xml_success_path": "s:Envelope/s:Body/CustSrchResponse/CustSrchRecArray/CustSrchRec/CustId",
    "request_body_spec": "{\n    CustSrch: {\n        TaxId: ((.loan_relations[0] | .tin) // null)\n    },\n    PersonName: {\n        ComName: ((.loan_relations[0] | .full_name) // null)\n    }\n}",
    "response_body_spec": "{ \n    apiSuccess: .apiSuccess,\n    errors: { \n        errorNum: (if (.errors.errorNum != \"\" and .errors.errorNum != null) then .errors.errorNum else null end),\n        errorMsg: ((if (.errors.errorNum != \"\" and .errors.errorNum != null) then .errors.errorMsg.Error else null end))\n    },\n    responses: (if .apiSuccess == true and .responses.Id \n        then [{ \n            cif: .responses.Id\n        }] \n        else null \n        end\n    )\n}",
    "request_body_spec_config": "{}",
    "response_body_spec_config": "{}"
}